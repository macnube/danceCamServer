# Prisma manages the following read-only fields:
#   id
#   createdAt
#   updatedAt

type Session {
  id: ID! @unique
  segments: [Segment!]!
  createdAt: DateTime!
  updatedAt: DateTime!
}


type Segment {
  id: ID! @unique
  name: String!
  videoId: String!
  startTime: Int!
  endTime: Int!
  mastery: Int # 0-5 - mastery of 0 expresses interest in wanting to learn the segment
  category: String # balboa, swing, etc
  tags: [String!]! # escape hatch for extra functionality
  createdAt: DateTime!
  updatedAt: DateTime!
}
